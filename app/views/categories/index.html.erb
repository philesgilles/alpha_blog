<h1>Categories</h1>
<div class="row d-flex justify-content-around">
    <div class="col-6 offset-3">
        <%= will_paginate%>
        <% @categories.each do |category| %>
            <p class=""><%=link_to "#{category.name}", category_path(category)%></p>
        <% end %>
        <%= will_paginate%>
    </div>
</div>